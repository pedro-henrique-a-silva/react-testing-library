{
  "name": "sd-0x-project-react-testing-library",
  "version": "1.1.0",
  "homepage": "https://pedro-henrique-a-silva.github.io/react-testing-library/",
  "private": true,
  "engines": {
    "node": "16 || 18"
  },
  "dependencies": {
    "gh-pages": "^6.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.10.0"
  },
  "scripts": {
    "dev": "vite --open",
    "build": "tsc & vite build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist --repo git@github.com:pedro-henrique-a-silva/react-testing-library.git",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint:styles": "npx stylelint '**/*.css'",
    "lint": "eslint --no-inline-config --no-error-on-unmatched-pattern -c .eslintrc.json . --ext .ts,.tsx"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@stryker-mutator/core": "^6.3.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@trybe/eslint-config-frontend": "^1.11.3",
    "@vitejs/plugin-react-swc": "^3.1.0",
    "@vitest/coverage-c8": "^0.30.1",
    "jsdom": "^21.1.0",
    "typescript": "^5.0.4",
    "vite": "^4.1.1",
    "vitest": "^0.30.1"
  }
}
